.ui-dialog {
  position: absolute;
  top: 0;
  background: white;
  -webkit-box-shadow: 0 0 5px rgba(0, 0, 0, 0.3);
  -moz-box-shadow: 0 0 5px rgba(0, 0, 0, 0.3);
  box-shadow: 0 0 5px rgba(0, 0, 0, 0.3);
}
.ui-dialog-title {
  display: none;
}
.ui-icon-closethick {
  display: block;
  position: absolute;
  right: -15px;
  top: -15px;
  width: 30px;
  height: 30px;
  background: #60605c;
  -webkit-border-radius: 100%;
  -moz-border-radius: 100%;
  border-radius: 100%;
  color: white;
  text-indent: -9999px;
  cursor: pointer;
}
.ui-icon-closethick:before {
  content: "X";
  display: block;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  line-height: 30px;
  color: white;
  z-index: 1;
  text-indent: 0;
  text-align: center;
  font-family: Georgia,serif;
}
.ui-widget-overlay {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: rgba(255, 255, 255, 0.75);
}
.ui-dialog-content {
  padding: 10px;
  overflow: hidden;
  overflow-y: auto;
}
.loading {
  background: url(../images/ajax-loading.gif) center center no-repeat;
}
@media only screen and (max-device-width: 500px) {
  .ui-icon-closethick {
    top: 10px;
    right: 10px;
  }
  .ui-dialog,
  .ui-dialog-content {
    width: 100% !important;
    height: auto !important;
    overflow: hidden !important;
  }
  .ajax-content-wrapper {
    width: 90% !important;
    height: auto !important;
    background: transparent !important;
    padding: 0 !important;
    margin: 0 !important;
  }
}
